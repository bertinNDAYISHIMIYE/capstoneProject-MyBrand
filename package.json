  "main": "index.js",
  "nyc": {
    "all": true,
    "reporter": [
      "text",
      "html",
      "teamcity"
    ],
    "exclude": [
      "coverage/**/*.js",
      "test/**/*.js",
      "UI"
    ]
  },
  "scripts": {
    "start": "node --require @babel/polyfill build/index.js",
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text mocha --require @babel/polyfill --require @babel/register 'server/**/**test.js'  --timeout 500000000 --exit",
